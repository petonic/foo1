#!/bin/bash

set -x


function finishProc {
  # Your cleanup code here
  echo "Cleaning up"
  killall -q /usr/bin/python chromium-browser python
}

trap finishProc EXIT SIGINT SIGTERM

projDir="$HOME/git/pithy"

cd "$projDir"

rubustat_web_interface.py >/dev/null &

# python tf.py &    # For debugging glask and web_interface issues


./runBrowser



